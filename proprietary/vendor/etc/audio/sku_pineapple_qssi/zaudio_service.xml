<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<zaudio_service>
<features>
    <feature name="MirrorMode" enable="true" mute_type="SW" sink_control="true"/>
    <feature name="MagicChat" enable="true" version="V1">
        <package_match>
            <package id="com.tencent.ig" pattern="FULL" desc="PUBGM"/>
            <package id="com.tencent.tmgp.sgame" pattern="FULL" desc="王者荣耀"/>
            <package id=".pubgmhd" pattern="PARTIAL" desc="和平精英"/>
            <package id=".miHoYo" pattern="PARTIAL" desc="米哈游"/>
            <package id=".speedmobile" pattern="PARTIAL" desc="QQ飞车"/>
            <package id=".lolm" pattern="PARTIAL" desc="英雄联盟"/>
            <package id=".cf" pattern="PARTIAL" desc="穿越火线"/>
        </package_match>
    </feature>
    <feature name="MagicVoice" enable="true" engine="YouMe"/>
    <feature name="PerfVolumeSuppression" enable="true">
        <target_devices>
            <device name="speaker" id="2">
                <section low="-58" high="-38" gain="-3"/>
                <section low="-38" high="-17" gain="-6"/>
                <section low="-17" high="0" gain="-9"/>
            </device>
        </target_devices>
    </feature>
    <feature name="Haptic4D" enable="true" />
</features>

<haptic4d_properties>
    <haptic4d_property game_name="测试软件" >
        <package_match>
            <package id="stone.audiotestsuite" pattern="FULL" desc="测试用"/>
        </package_match>
        <sample_rates>44100 24000 48000</sample_rates>
        <flags>AUDIO_OUTPUT_FLAG_DIRECT</flags>
        <mode>4</mode>
    </haptic4d_property>
    <haptic4d_property game_name="狂野飙车" >
        <package_match>
            <package id="com.gameloft.android.ANMP.Gloft" pattern="PARTIAL" desc="国际系列官服"/>
            <package id="kybc" pattern="PARTIAL" desc="其他渠道服"/>
        </package_match>
        <sample_rates>44100 24000 48000</sample_rates>
        <flags>AUDIO_OUTPUT_FLAG_FAST</flags>
        <mode>4</mode>
    </haptic4d_property>

    <haptic4d_property game_name="绝地求生" >
        <package_match>
            <package id="com.tencent.ig" pattern="FULL" desc="国际官服"/>
            <package id="com.tencent.iglite" pattern="FULL" desc="国际官服轻量版"/>
            <package id="com.vng.pubgmobile" pattern="FULL" desc="国际越南官服"/>
            <package id="com.pubg.krmobile" pattern="FULL" desc="国际日韩官服"/>
        </package_match>
        <sample_rates>44100 24000 48000</sample_rates>
        <flags>AUDIO_OUTPUT_FLAG_FAST</flags>
        <mode>3</mode>
        <notification_frame>-20</notification_frame>
    </haptic4d_property>

    <haptic4d_property game_name="荒野行动" >
        <package_match>
            <package id="com.netease.ko" pattern="PARTIAL" desc="国际官服"/>
            <package id="com.netease.hyxd" pattern="PARTIAL" desc="国内官服"/>
        </package_match>
        <sample_rates>44100 24000 48000</sample_rates>
        <flags>AUDIO_OUTPUT_FLAG_FAST</flags>
        <mode>1</mode>
        <notification_frame>-12</notification_frame>
    </haptic4d_property>

    <haptic4d_property game_name="城市飙车3D" >
        <package_match>
            <package id="com.racergame.cityracing3d" pattern="PARTIAL" desc="国际官服"/>
        </package_match>
        <sample_rates>44100 24000 48000</sample_rates>
        <flags>AUDIO_OUTPUT_FLAG_FAST</flags>
        <mode>7</mode>
        <notification_frame>-20</notification_frame>
    </haptic4d_property>

    <haptic4d_property game_name="QQ飞车" >
        <package_match>
            <package id="com.tencent.tmgp.speedmobile" pattern="PARTIAL" desc="国内官服"/>
        </package_match>
        <sample_rates>44100 24000 48000</sample_rates>
        <flags>AUDIO_OUTPUT_FLAG_FAST</flags>
        <mode>8</mode>
        <notification_frame>-20</notification_frame>
    </haptic4d_property>
</haptic4d_properties>

